 {
    "class": "Tenant",
    "Ansible-App-1": {
        "class": "Application",
        "template": "http",
        "serviceMain": {
            "class": "Service_HTTP",
            "virtualAddresses": [
                "{{virtual_ip}}"
            ],
            "pool": "web_pool"
        },
        "web_pool": {
            "class": "Pool",
            "monitors": [
                "http"
            ],
            "members": [{
                "servicePort": 8001,
                "serverAddresses": [
                    "{{ pool_mem_1 }}",
                    "{{ pool_mem_2 }}"
                ]
            }]
        }
    }
}
